{
  "name": "svelte-app",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "start": "sirv public --no-clear",
    "build:css": "tailwindcss build src/tailwind.css -o public/build/tailwind.css",
    "watch:css": "tailwindcss build src/tailwind.css -o public/build/tailwind.css --watch"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^24.0.0",
    "@rollup/plugin-node-resolve": "^15.0.0",
    "@rollup/plugin-terser": "^0.4.0",
    "autoprefixer": "^10.4.19",
    "postcss": "^8.4.38",
    "rollup": "^3.15.0",
    "rollup-plugin-css-only": "^4.3.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-svelte": "^7.1.2",
    "svelte": "^3.55.0",
    "svelte-preprocess": "^5.1.4",
    "svelte-spa-router": "^4.0.1",
    "tailwindcss": "^3.4.3"
  },
  "dependencies": {
    "sirv-cli": "^2.0.0"
  }
}
